{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pixel-editor-backend",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-28",
  "r2_buckets": [
    {
      "binding": "BLOB",
      "bucket_name": "my-bucket"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "my-database",
      "database_id": ""
    }
  ]
}